parameters:

database:
	dsn: 'mysql:host=%dbserver%;dbname=__DBNAME__'
	user: __DBUSER__
	password: __DBPASSWORD__

application:
	errorPresenter: Error
	mapping:
		*: App\*Module\Presenters\*Presenter


session:
	expiration: 14 days


di:
	export:
		parameters: no
		tags: no


services:
	- App\Router\RouterFactory::createRouter
